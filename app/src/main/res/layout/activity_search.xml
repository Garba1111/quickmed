<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".search">

<androidx.cardview.widget.CardView
    android:layout_width="match_parent"
    android:layout_height="wrap_content"
    app:cardCornerRadius="@dimen/_5dp"
    app:layout_constraintBottom_toBottomOf="parent"
    app:layout_constraintEnd_toEndOf="parent"
    app:layout_constraintStart_toStartOf="parent">


    <androidx.constraintlayout.widget.ConstraintLayout
        android:layout_width="match_parent"
        android:layout_height="380dp"
        android:background="@drawable/border2"
        >

        <androidx.cardview.widget.CardView
            android:id="@+id/cardView4"
            android:layout_width="match_parent"
            android:layout_height="@dimen/_50dp"
            android:layout_marginLeft="@dimen/_10dp"
            android:layout_marginTop="28dp"
            android:layout_marginRight="@dimen/_10dp"

            android:backgroundTint="#D6D6D6"
            android:elevation="@dimen/_20dp"
            app:cardCornerRadius="@dimen/_10dp"
            app:cardElevation="@dimen/_10dp"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintHorizontal_bias="0.0"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="parent">

            <TextView
                android:layout_width="match_parent"
                android:layout_height="@dimen/_50dp"
                android:gravity="center"
                android:background="@drawable/border"
                android:textColorHint="@color/black"
                android:textStyle="normal"
                android:fontFamily="@font/j"
                android:hint="Search Services"
                android:textSize="@dimen/_15dp"

                />

            <ImageView
                android:layout_width="@dimen/_30dp"
                android:layout_height="30dp"
                android:elevation="@dimen/_15dp"
                android:layout_gravity="right"
                android:layout_marginTop="@dimen/_10dp"
                android:layout_marginRight="@dimen/_15dp"

                android:src="@drawable/searchh" />

        </androidx.cardview.widget.CardView>

        <TextView
            android:layout_width="match_parent"
            android:layout_height="@dimen/_60dp"
            android:gravity="center"
            android:text="E- Consultant"
            android:paddingRight="@dimen/_125dp"
            android:background="@drawable/border"
            android:textSize="@dimen/_15dp"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintHorizontal_bias="1.0"
            app:layout_constraintStart_toEndOf="@+id/cardView5"
            tools:layout_editor_absoluteY="178dp" />

        <TextView
            android:id="@+id/textView9"
            android:layout_width="match_parent"
            android:layout_height="@dimen/_60dp"
            android:background="@drawable/border"
            android:gravity="center"
            android:paddingLeft="@dimen/_65dp"
            android:text="Ambulance Service"
            android:paddingRight="@dimen/_150dp"
            android:textSize="@dimen/_15dp"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintHorizontal_bias="1.0"
            app:layout_constraintStart_toEndOf="@+id/cardView5"
            tools:layout_editor_absoluteY="248dp" />

        <TextView
            android:id="@+id/textView34"
            android:layout_width="match_parent"
            android:layout_height="@dimen/_60dp"
            android:gravity="center"
            android:background="@drawable/border"
            android:text="Hospital Service"
            android:paddingRight="@dimen/_100dp"
            android:textSize="@dimen/_15dp"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintHorizontal_bias="0.868"
            app:layout_constraintStart_toEndOf="@+id/cardView5"
            tools:layout_editor_absoluteY="318dp" />

        <TextView
            android:id="@+id/textView3"
            android:layout_width="match_parent"
            android:layout_height="@dimen/_60dp"
            android:gravity="center"
            android:paddingRight="@dimen/_150dp"
            android:background="@drawable/border"
            android:text="Quick Call"

            android:textSize="@dimen/_15dp"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintHorizontal_bias="1.0"
            app:layout_constraintStart_toEndOf="@+id/cardView5"
            tools:layout_editor_absoluteY="108dp" />

        <androidx.cardview.widget.CardView
            android:id="@+id/cardView5"
            app:cardCornerRadius="@dimen/_30dp"
            android:layout_marginTop="@dimen/_30dp"
            android:layout_width="@dimen/_60dp"
            android:layout_height="@dimen/_60dp"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintHorizontal_bias="0.045"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/cardView4">
            <ImageView
                android:background="@color/main_color"
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:src="@drawable/ringervolume"/>
        </androidx.cardview.widget.CardView>

        <androidx.cardview.widget.CardView
            android:id="@+id/cardView6"
            android:layout_marginTop="@dimen/_10dp"
            android:layout_width="@dimen/_60dp"
            android:layout_height="@dimen/_60dp"
            app:cardCornerRadius="@dimen/_30dp"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintHorizontal_bias="0.045"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/cardView5">
            <ImageView
                android:background="@color/main_color"
                android:layout_width="match_parent"
                android:layout_height="match_parent"

                android:src="@drawable/nurse"
                />
        </androidx.cardview.widget.CardView>

        <androidx.cardview.widget.CardView
            android:id="@+id/cardView7"
            android:layout_marginTop="@dimen/_10dp"
            android:layout_width="@dimen/_60dp"
            android:layout_height="@dimen/_60dp"
            app:cardCornerRadius="@dimen/_30dp"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintHorizontal_bias="0.045"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/cardView6">
<ImageView
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:src="@drawable/amblance"
    android:background="@color/main_color"
    />
        </androidx.cardview.widget.CardView>

        <androidx.cardview.widget.CardView
            android:layout_width="@dimen/_60dp"
            android:layout_marginTop="@dimen/_10dp"
            android:layout_height="@dimen/_60dp"
            app:cardCornerRadius="@dimen/_30dp"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintHorizontal_bias="0.045"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/cardView7">
<ImageView
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@color/main_color"
    android:src="@drawable/ospital"/>
        </androidx.cardview.widget.CardView>

    </androidx.constraintlayout.widget.ConstraintLayout>

</androidx.cardview.widget.CardView>



  </androidx.constraintlayout.widget.ConstraintLayout>